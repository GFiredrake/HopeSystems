            <td class="rightTd">£@ViewBag.PredictedFreedomRevenue12</td>
        </tr>
        <tr>
            <td>Predicted New Freedom Customers</td>
            <td class="centerTd">@ViewBag.PredictedNewFreedomCustomer1</td>
            <td class="centerTd">@ViewBag.PredictedNewFreedomCustomer2</td>
            <td class="centerTd">@ViewBag.PredictedNewFreedomCustomer3</td>
