            <td class="centerTd">@ViewBag.PredictedCustomers11</td>
            <td class="centerTd">@ViewBag.PredictedCustomers12</td>
        </tr>
        <tr>
            <td>Predicted Freedom Customers</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers1</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers2</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers3</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers4</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers5</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers6</td>
