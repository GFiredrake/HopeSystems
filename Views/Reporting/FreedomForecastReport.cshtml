@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Freedom Forecast";

}

<h1 class="centeredText">Freedom Forecast Report</h1>

<div><h3>Number Of Current Customers: @ViewBag.NumberOfCustomers</h3></div>
<div><h3>Number Of Current Freedom Customers: @ViewBag.NumberOfFreedomCustomers</h3></div>
<div><h3>Freedom Members Percentage: @ViewBag.FreedomMembersPercentage%</h3></div>

<div id="swipe_text_wrap">
    <p class="swipe_text hide-desktop">&#8592;&nbsp;swipe the table below<br /> to view all content&nbsp;&#8594;</p>
</div>
<div id="table-scroll">
    <table>
        <tr>
            <td></td>
            <td class="centerTd">@ViewBag.MonthPlus1</td>
            <td class="centerTd">@ViewBag.MonthPlus2</td>
            <td class="centerTd">@ViewBag.MonthPlus3</td>
            <td class="centerTd">@ViewBag.MonthPlus4</td>
            <td class="centerTd">@ViewBag.MonthPlus5</td>
            <td class="centerTd">@ViewBag.MonthPlus6</td>
            <td class="centerTd">@ViewBag.MonthPlus7</td>
            <td class="centerTd">@ViewBag.MonthPlus8</td>
            <td class="centerTd">@ViewBag.MonthPlus9</td>
            <td class="centerTd">@ViewBag.MonthPlus10</td>
            <td class="centerTd">@ViewBag.MonthPlus11</td>
            <td class="centerTd">@ViewBag.MonthPlus12</td>
        </tr>
        <tr>
            <td>Predicted Customers</td>
            <td class="centerTd">@ViewBag.PredictedCustomers1</td>
            <td class="centerTd">@ViewBag.PredictedCustomers2</td>
            <td class="centerTd">@ViewBag.PredictedCustomers3</td>
            <td class="centerTd">@ViewBag.PredictedCustomers4</td>
