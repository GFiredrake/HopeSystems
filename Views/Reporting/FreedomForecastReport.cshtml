            <td class="centerTd">@ViewBag.PredictedFreedomCustomers11</td>
            <td class="centerTd">@ViewBag.PredictedFreedomCustomers12</td>
        </tr>
        <tr>
            <td>Predicted Freedom Revenue</td>
            <td class="rightTd">£@ViewBag.PredictedFreedomRevenue1</td>
            <td class="rightTd">£@ViewBag.PredictedFreedomRevenue2</td>
            <td class="rightTd">£@ViewBag.PredictedFreedomRevenue3</td>
